<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="TV"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Activity_TV"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SoundSettingsActivity"></activity>
        <activity android:name=".BluetoothSettingsActivity"></activity>
        <activity android:name=".AboutSettingsActivity"></activity>
        <activity android:name=".ProjectorSettingsActivity"></activity>
        <activity android:name=".ImageVideoSettingsActivity"></activity>
        <activity android:name=".SignalSourceSettingsActivity"></activity>
        <activity android:name=".SmartWallSettingsActivity"></activity>
        <activity android:name=".SystemSettingsActivity"></activity>
        <activity android:name=".NetworkSettingsActivity"></activity>
    </application>

</manifest>